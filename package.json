{
  "name": "fuzzy-searcher",
  "version": "1.0.6",
  "description": "Small, well tested fuzzy search for node or browser",
  "main": "./dist/bundle.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "webpack --env target='web'",
    "increment": "npm version patch",
    "build:web": "webpack --env target='web'",
    "test": "TS_NODE_PROJECT='tsconfig.json' mocha -w",
    "test:ci": "TS_NODE_PROJECT='tsconfig.json' mocha",
    "coverage": "nyc --reporter=text mocha",
    "coverage:ci": "nyc --reporter=html mocha"
  },
  "pre-push": [
    "increment",
    "test:ci"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Frankeo/fuzzy-search.git"
  },
  "author": "Francisco Moreno",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Frankeo/fuzzy-search/issues"
  },
  "homepage": "https://github.com/Frankeo/fuzzy-search#readme",
  "devDependencies": {
    "@types/mocha": "^9.1.0",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "pre-push": "^0.1.2",
    "ts-loader": "^9.2.9",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-webpack-plugin": "^2.1.1",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  }
}
